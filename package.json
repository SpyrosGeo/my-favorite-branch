{
  "name": "my-favorite-branch",
  "displayName": "My favorite branch ",
  "description": "A simple extention that lets you favorite git branches for easer switching",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.77.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "my-favorite-branch.helloWorld",
        "title": "FavoriteBranch: toggle"
      }
    ],
    "statusbar": {
    "command": "extension.openBranchSelector",
    "text": "$(git-branch) $(git-symbol) $(git-branch)",
    "tooltip": "Select Branch"
},
"viewsContainers": {
      "explorer": [
        {
          "id": "myExtension.branchSelector",
          "title": "Branch Selector"
        }
      ]
    },
    "views": {
      "myExtension.branchSelector": [
        {
          "id": "myExtension.branchList",
          "name": "Branch List",
          "when": "resourceScheme == 'file'"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.77.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "eslint": "^8.39.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.0.4",
    "@vscode/test-electron": "^2.3.0"
  }
}
